<!DOCTYPE html>
<html>
<head>
    <link rel="shortcut icon" type="image/png" href="favicon.png">
    <link rel="apple-touch-icon" href="favicon.png">
    <title> @ViewData["DomainName"] - Maintenance</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="~/css/site.css?v=1" type="text/css" rel="stylesheet">
</head>
<body>
    <div id="wrapper">
        <div class="backbar"></div>
        <div id="container">
            @RenderBody()
        </div>
    </div>
    <div id="footer">
        <span><a href="@ViewData["CompanyUrl"]">@ViewData["CompanyName"]</a> monotoring system powered by <a href="https://github.com/Acczad/Owleye">owleye</a></span>
    </div>

</body>
</html>
